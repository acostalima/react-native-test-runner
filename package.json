{
  "name": "react-native-test-runner",
  "version": "1.0.3",
  "description": "Run tests in React Native's environment.",
  "repository": "acostalima/react-native-test-runner",
  "author": {
    "name": "AndrÃ© Costa Lima",
    "email": "andreclima.pt@gmail.com",
    "url": "https://andrecostalima.dev"
  },
  "license": "MIT",
  "keywords": [
    "react-native",
    "test",
    "unit",
    "integration",
    "testing",
    "zora",
    "ios",
    "android"
  ],
  "engines": {
    "node": ">=12"
  },
  "bin": {
    "rn-test": "./cli/index.js"
  },
  "files": [
    "src",
    "cli",
    "android",
    "ios",
    "babel.config.js",
    "postinstall.js",
    ".watchmanconfig",
    "patches",
    "!build",
    "!ios/Pods",
    "!xcuserdata",
    "!*.xcuserstate",
    "!.idea",
    "!.gradle",
    "!*.iml",
    "!local.properties"
  ],
  "scripts": {
    "ios:debug": "react-native run-ios --configuration Debug",
    "android:debug": "react-native run-android --variant debug",
    "metro": "react-native start",
    "lint": "eslint --cache --ignore-path .gitignore *.js",
    "pods": "pod-install",
    "xcode": "xed -b ios",
    "studio": "studio android",
    "postinstall": "./postinstall.js",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin HEAD && conventional-github-releaser -p angular",
    "test": "jest"
  },
  "dependencies": {
    "execa": "^4.1.0",
    "globby": "^11.0.1",
    "is-ci": "^2.0.0",
    "is-uuid": "^1.0.2",
    "meow": "^8.0.0",
    "ora": "^5.1.0",
    "p-retry": "^4.2.0",
    "patch-package": "^6.2.2",
    "pico-signals": "^1.0.0",
    "process-exists": "^4.0.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "tempy": "^1.0.0",
    "yn": "^4.0.0",
    "zora": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-async-generator-functions": "^7.12.1",
    "@babel/runtime": "^7.8.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@moxy/eslint-config-babel": "^13.0.3",
    "@moxy/eslint-config-base": "^13.0.3",
    "@moxy/eslint-config-jest": "^13.0.3",
    "@moxy/eslint-config-react-native": "^13.0.3",
    "@moxy/jest-config-base": "^5.0.0",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "conventional-github-releaser": "^3.1.5",
    "eslint": "^6.8.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.5.2",
    "metro-react-native-babel-preset": "^0.59.0",
    "pod-install": "^0.1.12",
    "prettier": "^2.2.1",
    "read-pkg-up": "^7.0.1",
    "standard-version": "^9.0.0",
    "tap-parser": "^10.1.0"
  }
}
